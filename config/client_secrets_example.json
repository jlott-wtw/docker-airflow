{
    "web": {
        "issuer": "https://{auth0_tenant}.auth0.com/",
        "client_id": "{clientId}",
        "client_secret": "{secret}",
        "redirect_uris": [
            "http://localhost:8080/oidc_callback"
        ],
        "auth_uri": "https://{auth0_tenant}.auth0.com/authorize",
        "token_uri": "https://{auth0_tenant}.auth0.com/oauth/token",
        "userinfo_uri": "https://{auth0_tenant}.auth0.com/userinfo"
    }
}